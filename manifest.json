{
  "manifest_version": 3,
  "name": "GoldEye",
  "version": "1.1",
  "description": "轻量级黄金行情浏览器插件，一键查看实时金价。支持自定义URL。",
  "permissions": ["activeTab", "tabs", "storage", "contextMenus"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "options_page": "options/options.html",
  "background": {
    "service_worker": "background/background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
